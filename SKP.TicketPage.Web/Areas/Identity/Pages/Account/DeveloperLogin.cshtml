@page
@model DeveloperLoginModel
@{
    ViewData["Title"] = "Developer login";
}

<h1>Udvikler Login</h1>
<div class="row">
    <div class="col-md-4">
        <section>
            <form id="account" method="post">
                <h4>Brug test konto til login</h4>
                <hr />
                <div asp-validation-summary="All" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Input.Email"></label>
                    <input asp-for="Input.Email" class="form-control cust-border-box" />
                </div>
                <div class="form-group">
                    <label asp-for="Input.Password">Adgangskode</label>
                    <input asp-for="Input.Password" class="form-control cust-border-box" />
                </div>
                <div class="form-group">
                    <button type="submit" class="btn cust-bg-sblue text-white border cust-border-box">Log ind</button>
                </div>
            </form>
        </section>
    </div>
</div>

@section Scripts {
<partial name="_ValidationScriptsPartial" />
}
